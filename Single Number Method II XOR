//  Method II using XOR Concept
//  Algorithm explained : https://www.youtube.com/watch?v=CkF9KMZXElA&list=PLzsSZcqdlUAPnRCe5ZMLqYF9hLy6NOzmw&index=2

class Solution {
    public int singleNumber(int[] nums) {
        int result = 0;
        for(int i : nums)
            result ^= i;
        return result;
    }
   }
